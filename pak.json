{
    "name": "esp-html-skeleton",
    "description": "ESP Server-Side HTML MVC",
    "version": "6.0.0",
    "keywords": [
        "esp"
    ],
    "licenses": [
        {
            "type": "GPL",
            "url": "http://www.gnu.org/licenses/gpl-2.0.html"
        },
        {
            "type": "Commercial",
            "url": "https://embedthis.com/licensing/"
        }
    ],
    "dependencies": {
        "embedthis/esp-mvc": "~6"
    },
    "devDependencies": {
        "pak": ">=1.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/esp-html-skeleton.git"
    },
    "ignore": [
        "test**"
    ],
    "blend": {
        "devDependencies": {
            "?esp": ">=8.0"
        },
        "directories": {
            "?export": "contents/lib"
        }
    },
    "export": {
        "from": [
            "contents/**",
            "layouts/**",
            "partials/**"
        ],
        "to": "${TOP}",
        "overwrite": false
    },
    "manage": [
        "esp.json",
        "expansive.json"
    ],
    "origin": "embedthis/esp-html-skeleton"
}
