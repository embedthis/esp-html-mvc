/*
    expansive.json - Configuration for esp-html-skeleton
 */
{
    debug: {
        services: {
            "?compress": false,
            "?html": false,
            "?js": {
                "?minify": false,
                "?usemin": true,
                "?usemap": true
            }
        }
    },
    release: {
        services: {
            "?compress": true,
            "css": {
                "?minify": true,
                "?usemin": true,
                "?usemap": false
            },
            "js": {
                "?minify": true,
                "?usemin": true,
                "?usemap": false
            }
        }
    }
}
