{
    "name": "esp-html-skeleton",
    "description": "ESP Server-Side HTML MVC",
    "version": "5.5.0",
    "keywords": [
        "esp"
    ],
    "licenses": [
        {
            "type": "GPL",
            "url": "http://www.gnu.org/licenses/gpl-2.0.html"
        },
        {
            "type": "Commercial",
            "url": "https://embedthis.com/licensing/"
        }
    ],
    "dependencies": {
        "esp-mvc": "~5.5"
    },
    "devDependencies": {
        "pak": ">=0.11.3"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/esp-html-skeleton.git"
    },
    "ignore": [
        "test**"
    ],
    "pak": {
        "blend": {
            "devDependencies": {
                "?esp": "~6.x"
            }
        },
        "export": {
            "from": [
                "contents/**",
                "layouts/**",
                "partials/**"
            ],
            "to": "${TOP}",
            "overwrite": false
        },
        "manage": [
            "esp.json"
        ]
    }
}
